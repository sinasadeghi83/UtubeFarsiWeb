{
  "name": "floor12/yii2-phone",
  "description": "Yii2 phone formatter and validator.",
  "type": "yii2-component",
  "minimum-stability": "stable",
  "keywords": [
    "floor12",
    "phone",
    "validator",
    "formatter"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "floor12@floor12.net",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "~2.0.13"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "floor12\\phone\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\phone\\tests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
